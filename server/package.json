  "name": "server",
  "private": true,
  "scripts": {
    "build": "npm run clean && nest build && npm run copy-config && zip -r dist.zip dist && open .",
    "clean": "rimraf dist",
    "clean-all": "rimraf dist && rimraf node_modules ",
    "copy-config": "cp .application.yml dist/; cp -a src/views dist/src",
    "copy-config-windows": "copy .application.yml dist\\ && xcopy /E /I /Y src\\views dist\\src\\",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prestart:prod": "npm run build",
    "start": " nest start",
    "start:debug": "TZ=UTC cross-env NODE_ENV=dev NODE_OPTIONS=--max_old_space_size=3000 nest start --debug --watch",
    "start:debugWin": "set TZ=UTC && cross-env NODE_ENV=dev NODE_OPTIONS=--max_old_space_size=3000 nest start --debug --watch",
    "start:debug-org": " nodemon --config nodemon-debug.json",
    "start:dev": "concurrently nodemon \"tsc -w -p tsconfig.build.json\"",
    "start:prod": "cross-env NODE_ENV=prod  node dist/src/main.js",
    "test": "cross-env NODE_ENV=dev jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
